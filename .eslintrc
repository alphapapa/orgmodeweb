{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "globals": {
        "$": true,
        "ORG": true,
        "Dropbox": true,
        "QUnit": true
    },
    "root": true,
    "rules": {
        "consistent-return": "error",
        "curly": [
            2,
            "multi-line"
        ],
        "eqeqeq": "warn",
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "max-len": "off",
        "new-cap": "off",
        "no-extend-native": "error",
        "no-invalid-this": "off",
        "no-throw-literal": "off",
        "no-var": "off",
        "prefer-rest-params": "off",
        "quotes": [
            2,
            "double"
        ],
        "semi": "error",
        "space-before-function-paren": 2,
        "strict": 0
    }
}